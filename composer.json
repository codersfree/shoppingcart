{
    "name": "codersfree/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": [
        "laravel",
        "shoppingcart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Arana",
            "email": "victor@codersfree.com"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11.0|^12.0",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11.0|^12.0",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~10.0 || ~11.0",
        "mockery/mockery": "~0.9.0 || ~1.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "CodersFree\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodersFree\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "codersfree/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "CodersFree\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "CodersFree\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}